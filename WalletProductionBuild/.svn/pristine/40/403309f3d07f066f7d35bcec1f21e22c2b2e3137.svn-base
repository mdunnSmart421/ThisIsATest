/**** PROGRESS MARKERS (MAIN STEPS) *****/
.progress-steps {
    background-color: #121A3C;
    background: linear-gradient(top, rgb(11, 66, 123), rgb(19, 26, 60), color-stop(50%, rgb(22, 40, 85)), color-stop(50%, rgb(18, 26, 60)));
    background: -webkit-linear-gradient(top, rgb(11, 66, 123), rgb(19, 26, 60), color-stop(50%, rgb(22, 40, 85)), color-stop(50%, rgb(18, 26, 60)));
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(11, 66, 123)), to(rgb(19, 26, 60)), color-stop(50%, rgb(22, 40, 85)), color-stop(50%, rgb(18, 26, 60)));
    background: -moz-linear-gradient(top, rgb(11, 66, 123), rgb(19, 26, 60) 50%, rgb(22, 40, 85) 50%, rgb(18, 26, 60));
    background: -o-linear-gradient(top, rgb(11, 66, 123), rgb(19, 26, 60) 50%, rgb(22, 40, 85) 50%, rgb(18, 26, 60));
    display: inline-block;
    width: 100%;
    /*-moz-box-shadow: 0 1px 0 0 #035B98, 0 2px 0 0 #000;*/
    /*-webkit-box-shadow: 0 1px 0 0 #035B98, 0 2px 0 0 #000;*/
    /*box-shadow: 0 1px 0 0 #035B98, 0 2px 0 0 #000;*/
    font-size: 1em;
    color: #76859C
}

.progress-steps span {
    padding: 4px 12px;
    -webkit-border-top-right-radius: 20px 20px;
    -webkit-border-bottom-right-radius: 20px 20px;
    -moz-border-radius-topright: 20px;
    -moz-border-radius-bottomright: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    -moz-box-shadow: inset -2px 0 #0E1935;
    display: inline-block;
    float: left;
    -webkit-box-shadow: inset -2px 0 #0E1935;
    box-shadow: inset -2px 0 #0E1935;
    text-shadow: 0px -1px 0px #000;

}

.progress-steps span.active {
    background-color: #3A8ABD;
    /*background-color: red;*/

    background: linear-gradient(top, rgb(34, 67, 132), rgb(25, 54, 116), color-stop(50%, rgb(32, 63, 129)), color-stop(50%, rgb(25, 54, 116)));
    background: -webkit-linear-gradient(top, rgb(34, 67, 132), rgb(25, 54, 116), color-stop(50%, rgb(32, 63, 129)), color-stop(50%, rgb(25, 54, 116)));
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(34, 67, 132)), to(rgb(25, 54, 116)), color-stop(50%, rgb(32, 63, 129)), color-stop(50%, rgb(25, 54, 116)));
    background: -moz-linear-gradient(top, rgb(34, 67, 132), rgb(25, 54, 116) 50%, rgb(32, 63, 129) 50%, rgb(25, 54, 116));
    background: -o-linear-gradient(top, rgb(34, 67, 132), rgb(25, 54, 116) 50%, rgb(32, 63, 129) 50%, rgb(25, 54, 116));
    color: #FFF
}

.progress-steps span:first-child {
    position: relative;
    z-index: 20;

}

.progress-steps #step-two {
    position: relative;
    left: -20px;
    text-indent: 12px;
    z-index: 19;

}

.progress-steps span:last-child {
    position: relative;
    left: -25px;

    z-index: 18;
    /*background-color: red;*/
}

/**** PROGRESS MARKERS (SUB STEPS) *****/
.progress-substeps {
    position: absolute;
    top: 1px;
    right: 0;
    padding: 8px;
    -webkit-border-bottom-left-radius: 6px 6px;;
    -moz-border-radius-bottomleft: 6px;
    border-bottom-left-radius: 6px;
    box-shadow: 0 2px 6px #131E42;
    -moz-box-shadow: 0 2px 6px #131E42;
    -webkit-box-shadow: 0 2px 6px #131E42;
    border-left: 1px solid #191A1B;
    border-bottom: 1px solid #191A1B;
    background: linear-gradient(top, rgba(20, 43, 90, 0.3), rgba(20, 41, 83, 0.3));
    background: -webkit-linear-gradient(top, rgba(20, 43, 90, 0.3), rgba(20, 41, 83, 0.3));
    background: -webkit-gradient(linear, left top, left bottom, from(rgba(20, 43, 90, 0.3)), to(rgba(20, 41, 83, 0.3)));
    background: -moz-linear-gradient(top, rgba(20, 43, 90, 0.3), rgba(20, 41, 83, 0.3));
    background: -o-linear-gradient(top, rgba(20, 43, 90, 0.3), rgba(20, 41, 83, 0.3))
}

.progress-substeps span {
    border-radius: 8px;
    background: #485C84;
    float: left;
    margin-right: 8px;
    height: 14px;
    width: 14px;
    box-shadow: 0 2px 6px #131E42;
    -moz-box-shadow: 0 2px 6px #131E42;
    -webkit-box-shadow: 0 2px 6px #131E42
}

.progress-substeps span.active {
    background: #D3E9FE
}

.progress-substeps span:last-child {
    margin: 0
}

