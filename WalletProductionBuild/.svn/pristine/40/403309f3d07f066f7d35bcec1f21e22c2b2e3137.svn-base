/***** SELECT BOX ******/
.content select {
    border: 0;
    outline: 0;
    display: inline;
    overflow: hidden;
    width: 100%;
    height: 40px;
    padding-left: 8px;
    line-height: 40px;
    -webkit-appearance: none;
    -moz-appearance: none;
    background: transparent;
    font-size: 1em;
    color: #333;
}

.content h1 {
    color: white;
}

.content div.static {
    font-size: 15px;
    font-weight: bold;
    color: white;
    margin-left: 11px;
    padding-bottom: 15px;
}

.select-wrapper {
    height: 20px;
    background-color: #F6F6F7;
    border-radius: 8px;
    box-shadow: 0 2px 6px #131E42;
    -moz-box-shadow: 0 2px 6px #131E42;
    -webkit-box-shadow: 0 2px 6px #131E42;
    height: 40px;
    line-height: 40px;
    position: relative;
    margin-bottom: 14px;
}

.arrow:before {
    content: '\25bc';
    position: absolute;
    right: 0;
    top: 0;
    width: 40px;
    height: 40px;
    display: block;
    text-align: center;
    -webkit-border-top-right-radius: 8px 9px;
    -webkit-border-bottom-right-radius: 8px 8px;
    -moz-border-radius-topright: 8px;
    -moz-border-radius-bottomright: 8px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    background-color: #3A8ABD;
    background: linear-gradient(top, rgb(81, 175, 228), rgb(58, 139, 189), color-stop(50%, rgb(76, 166, 219)), color-stop(50%, rgb(65, 150, 200)));
    background: -webkit-linear-gradient(top, rgb(81, 175, 228), rgb(58, 139, 189), color-stop(50%, rgb(76, 166, 219)), color-stop(50%, rgb(65, 150, 200)));
    background: -webkit-gradient(linear, left top, left bottom, from(rgb(81, 175, 228)), to(rgb(58, 139, 189)), color-stop(50%, rgb(76, 166, 219)), color-stop(50%, rgb(65, 150, 200)));
    background: -moz-linear-gradient(top, rgb(81, 175, 228), rgb(58, 139, 189) 50%, rgb(76, 166, 219) 50%, rgb(65, 150, 200));
    background: -o-linear-gradient(top, rgb(81, 175, 228), rgb(58, 139, 189) 50%, rgb(76, 166, 219) 50%, rgb(65, 150, 200));
    -moz-box-shadow: inset 2px 0 2px -2px #1E1E1E;
    -webkit-box-shadow: inset 2px 0 2px -2px #1E1E1E;
    box-shadow: inset 2px 0 2px -2px #1E1E1E;
    text-shadow: 0px 1px 0 #000
}

