//----------------------------------------------------------------------
//Function:     resetDD
//Description:  blanks the Day field when invoked
//              
//Parameters:   inField:  the object referring to the Day field
//Return value: 
//-----------------------------------------------------------------------
function resetDD(inField)
{
	if (inField.value=="DD")
	{
		inField.value="";
    }
}
      
//----------------------------------------------------------------------
//Function:     resetYYYY
//Description:  blanks the Year field when invoked
//            
//Parameters:   inField:  the object referring to the Year field
//Return value: 
//-----------------------------------------------------------------------
function resetYYYY(inField)
{
	if (inField.value=="YYYY")
	{
		inField.value="";
    }
}